{
	"name": "zeronetwork",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev --turbopack",
		"build": "npx prisma generate && npx prisma migrate dev && next build",
		"start": "next start",
		"lint": "next lint",
		"docker": "docker-compose up -d",
		"prisma": "npx prisma generate && npx prisma migrate dev",
		"studio": "npx prisma studio",
